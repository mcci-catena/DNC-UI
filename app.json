{
  "expo": {
    "name": "DNC",
    "slug": "DNC",
    "version": "1.0.0",
    "orientation": "default",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
