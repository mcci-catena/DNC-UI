      "bundleIdentifier": "com.smppandi.DNC"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo@2.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.smppandi.DNC"
    },
    "web": {
      "favicon": "./assets/logo@2.png"
    }
  }
}
